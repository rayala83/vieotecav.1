@media (max-width: 767px) {
	
	#menu.navbar-default {
		background-color: rgba(0, 0, 0, 0.79) !important;
    }
	
	a#text-barmenu{
        display: inline-block;
    }
	
}
@media (min-width: 768px) {
	
	.navbar .navbar-nav {
        display: inline-block;
        float: none;
        vertical-align: top;
    }
	
	.navbar .navbar-collapse {
        text-align: center;
    }
	
	a#text-barmenu{
        display: none;
    }
	
}

/* Inicio video responsive */

@media(min-width: 200px){
    .video-container {
        background: url('../img/fondos/05.jpg') no-repeat center center; 
        -webkit-background-size: contain;
        -moz-background-size: contain;
        -o-background-size: contain;
        background-size: contain;
        position: relative;
        padding-bottom: 84px;
        padding-top: 5px;
        padding-left: 10px;
        padding-right: 10px;
        height: 250px;
        width: 300px;
        display: inline-block;
